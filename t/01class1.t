use strict;
use Test::More tests => 77;

use_ok('Number::Phone::JP', 'class1');

my $tel  = Number::Phone::JP->new;
my $line = 52;

for (1..$line) {
	my $num = <DATA>;
	chomp $num;
	ok($tel->set_number($num)->is_valid_number);
}

while (<DATA>) {
	chomp;
	ok(! $tel->set_number($_)->is_valid_number);
}

