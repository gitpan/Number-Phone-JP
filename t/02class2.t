use strict;
use Test::More tests => 29;

use_ok('Number::Phone::JP', 'class2');

my $tel  = Number::Phone::JP->new;
my $line = 18;

for (1..$line) {
	my $num = <DATA>;
	chomp $num;
	ok($tel->set_number($num)->is_valid_number);
}

while (<DATA>) {
	chomp;
	ok(! $tel->set_number($_)->is_valid_number);
}

